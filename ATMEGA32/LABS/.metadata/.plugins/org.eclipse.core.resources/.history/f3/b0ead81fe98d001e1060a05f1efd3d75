	#endif

	/* 2- S*/
	#if		SPI_NODE_TYPE == MASTER

	#elif	SPI_NODE_TYPE == SLAVE

	#else
	#error	Wrong SPI_NODE_TYPE Configuration Option
	#endif

	/* 3- */
	#if		SPI_NODE_TYPE == MASTER

	#elif	SPI_NODE_TYPE == SLAVE

	#else
	#error	Wrong SPI_NODE_TYPE Configuration Option
	#endif

	/* 4- Interrupt Enable */
	#if		SPI_INTERRUPT_STATUS == ENABLED
			/* Don't forget to enable the global interrupt */
			SE

	#elif	SPI_NODE_TYPE == SLAVE

	#else
	#error	Wrong SPI_NODE_TYPE Configuration Option
	#endif


}


